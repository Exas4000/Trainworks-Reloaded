{
    "$schema": "./json-schema.json",
    "map_nodes": [
        {
            "id": "MyNode",
            "type": "reward",
            "map_icon": "@StewardIconSmall",
            "minimap_icon": "@StewardIconSmall",
            "prefab": "@StewardMapIcon",
            "buckets": [
                {
                    "run_type": "primary",
                    "bucket": "Banners Ring 1"
                }
            ],
            "map_pools": [
                "RandomChosenMainClassUnit",
                "RandomChosenSubClassUnit"
            ],
            "extensions": [
                {
                    "reward": {
                        "required_class": "@ClassSteward",
                        "rewards": [
                            {
                                "id": "@StewardBannerReward"
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "card_pools": [
        {
            "id": "StewardBannerCardPool",
            "cards": ["@SpawnBiggerSteward"]
        }
    ],
    "rewards": [
        {
            "id": "StewardBannerReward",
            "type":"cardpool",
            "extensions": [
                {
                    "cardpool": {
                        "cardpool": "@StewardBannerCardPool"
                    }
                }
            ]
        }
    ],
    "game_objects": [
        {
            "id": "StewardMapIcon",
            "type":"map_node_icon",
            "extensions": {
                "map_node_icon": {
                    "disabled_sprite": "@StewardBanner",
                    "enabled_sprite": "@StewardBanner",
                    "frozen_sprite": "@StewardBanner",
                    "visited_sprite_disabled": "@StewardBanner",
                    "visited_sprite_enabled": "@StewardBanner"
                }
            }
        },
            
        {
            "id": "OverclockArt",
            "type": "card_art",
            "extensions": {
                "card_art": {
                    "sprite": "@OverclockArt"
                }
            }
        }
    ]
}